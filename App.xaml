<?xml version="1.0" encoding="UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:converters="clr-namespace:ProjectMaui.Converters"
             x:Class="ProjectMaui.App">
    <Application.Resources>
        <ResourceDictionary>
            <converters:InvertedBoolConverter x:Key="InvertedBoolConverter"/>
            <converters:StringNotEmptyConverter x:Key="StringNotEmptyConverter"/>
            <converters:StatusToCancelButtonVisibilityConverter x:Key="StatusToCancelButtonVisibilityConverter"/>
            <converters:DayOfWeekConverter x:Key="DayOfWeekConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>